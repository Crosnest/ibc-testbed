[Unit]
Description=Lum Network daemon
After=network-online.target

[Service]
User=root
ExecStart=/root/go/bin/lumd start --p2p.laddr tcp://0.0.0.0:10001 --rpc.laddr tcp://127.0.0.1:10002  --home /root/.ibc-testbed/lumd
Restart=on-failure
RestartSec=3
LimitNOFILE=4096

[Install]
WantedBy=multi-user.target
