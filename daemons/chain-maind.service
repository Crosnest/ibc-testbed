[Unit]
Description=crypto-org-chain daemon
After=network-online.target

[Service]
User=root
ExecStart=/home/dpierret/go/bin/chain-maind start --minimum-gas-prices "0.0basecro" --p2p.laddr tcp://0.0.0.0:26656 --rpc.laddr tcp://127.0.0.1:26657 --address tcp://0.0.0.0:26658 --grpc.address 0.0.0.0:26090 --grpc-web.address 0.0.0.0:26091 --home /home/dpierret/.ibc-testbed/chain-maind
