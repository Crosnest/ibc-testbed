[Unit]
Description=Osmosis daemon
After=network-online.target

[Service]
User=root
ExecStart=/root/go/bin/osmosisd start --p2p.laddr tcp://0.0.0.0:20001 --rpc.laddr tcp://127.0.0.1:20002 --home /root/.ibc-testbed/osmosisd
Restart=on-failure
RestartSec=3
LimitNOFILE=4096

[Install]
WantedBy=multi-user.target
